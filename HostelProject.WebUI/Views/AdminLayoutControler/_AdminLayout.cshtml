
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("HeadPartial")

<body>

    @await Html.PartialAsync("PreLoaderPartial")

    <div id="main-wrapper">

        @await Html.PartialAsync("NavHeaderPartial")

        @await Html.PartialAsync("HeaderPartial")

        @await Html.PartialAsync("SideBarPartial")

        <div class="content-body">


            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>


        @await Html.PartialAsync("FooterPartial")

    </div>

    @await Html.PartialAsync("ScriptPartial")
</body>

</html>