@using HostelProject.WebUI.Dtos.GuestDto
@model CreateGuestDto

@{
    ViewData["Title"] = "AddGuest";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form method="post">
    <label>Misafir  Adı</label>
    <input type="text" asp-for="Name" class="form-control" />
    <br />
    <label>Misafir  Soyadı</label>
    <input type="text" asp-for="Surname" class="form-control" />
    <br />
    <label>Şehiri</label>
    <input type="text" asp-for="City" class="form-control" />
    <br />


    <button class="btn btn-primary">Misafir Ekle</button>
</form>

